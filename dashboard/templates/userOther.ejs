<%- include("partials/header", { bot, user, path, title: `User | ${reqUser.username}` }) %>


<div class="container-sm center" style="margin-top: 25px;">
  <div class="d-grid gap-2 col-6 mx-auto">
    <h2 style="text-align: center;">
        <img src=<%= `https://cdn.discordapp.com/avatars/${reqUser.id}/${reqUser.avatar}` %> alt=<%= `${reqUser.username}'s avatar` %> style="width:96px;height:96px;">
        <%= reqUser.username %>#<%= reqUser.discriminator %>
    </h2>
    <div class="wrap">
    
    </div>
    <h1 style="text-align: center;">Votes: <%- results.voteTotal %> Blacklisted: <%- bot.convertBoolean(results.blacklisted) %> </h1>
  </div>
</div>

<%- include("partials/footer") %>